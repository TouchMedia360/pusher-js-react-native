{
  "compilerOptions": {
    "strict": false,
    "sourceMap": true,
    "module": "ES6",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "ES6",
    "types": ["node"],
    "removeComments": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "*": [
        "*",
        "runtimes/*"
      ]
    },
    "lib": [
      "ScriptHost",
      "DOM",
      "ES6"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
